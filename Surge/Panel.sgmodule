#!name=Panels
#!desc=信息面板

[Panel]
#Surge Pro标题,可显示启动时间,点击刷新为重载配置
SurgePro_ReloadProfile = script-name=SurgePro_ReloadProfile,update-interval=1

#网络状态
net-info-panel=title="网络状态",content="请刷新",style=info,script-name=net-info-panel

#网络详情
NET_info = script-name=NET_info,update-interval=1

#Youtube解锁检测
youtube_premium_check = script-name=youtube_premium_check, title="YouTube 解锁检测", update-interval=1

#Netflix解锁检测
 nf_check = script-name=nf_check, title="Netflix 解锁检测", content="请刷新", update-interval=1

#Disney+解锁检测
 disney_check = script-name=disney_check,title=Disney+ 解锁检测,style=alert,content=请刷新面板,update-interval=1800
 

[Script]
#附带重载配置,Surge Pro标题,可显示启动时间,点击刷新为重载配置
SurgePro_ReloadProfile = type=generic,timeout=10,script-path= https://raw.githubusercontent.com/oreoleeus/Master/main/Surge/Scripts/surgepro_reloadprofile.js ,argument=icon=crown.fill&color=#f6c970

# Surge 网络信息
net-info-panel=script-path=https://raw.githubusercontent.com/oreoleeus/Master/main/Surge/Scripts/Net.js,type=generic

#应当修改的字段 group 代理策略组名称
NET_info = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/oreoleeus/Master/main/Surge/Scripts/net_info.js,argument=icon=externaldrive.connected.to.line.below&color=#9a7ff7&group=🌈 Proxy

#YouTube 解锁检测
youtube_premium_check = type=generic, script-path=https://raw.githubusercontent.com/oreoleeus/Master/main/Surge/Scripts/youtube_premium_check.js, argument=title=YouTube 解锁检测

#Netflix 解锁检测
nf_check = type=generic, script-path=https://raw.githubusercontent.com/oreoleeus/Master/main/Surge/Scripts/nf_check.js, argument=title=Netflix 解锁检测

#Disney+ 解锁检测
disney_check = type=generic,script-path=https://raw.githubusercontent.com/oreoleeus/Master/main/Surge/Scripts/disney_check.js, argument=title=Disney 解锁检测
